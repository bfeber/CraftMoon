[sub_resource type="GDScript" id="GDScript_mhx1m"]
script/source = "extends RayCast3D


const MOON_MATERIAL = preload(\"res://materials/concrete/concrete.tres\")


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	if Input.is_action_just_pressed(&\"destroy\"):
		var level_portal_collider := get_collider()
		if level_portal_collider:
			var level_portal := level_portal_collider.get_parent() as LevelPortal
			level_portal.menu.delete_save(level_portal.level)
			level_portal.level = \"\"
			level_portal.label.text = \"\"
			level_portal.cylinder.material = MOON_MATERIAL
"

